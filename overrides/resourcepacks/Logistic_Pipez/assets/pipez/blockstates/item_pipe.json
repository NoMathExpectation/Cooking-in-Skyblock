{
  "multipart": [
      {
          "when": {
              "north": "true",
      "south": "false"
          },
          "apply": {
              "model": "pipez:block/item/node"
          }
      },
      {
          "when": {
              "south": "true",
      "north": "false"
          },
          "apply": {
              "model": "pipez:block/item/node",
              "y": 180
          }
      },
      {
          "when": {
              "east": "true",
      "west": "false"
          },
          "apply": {
              "model": "pipez:block/item/node",
              "y": 90
          }
      },
      {
          "when": {
              "west": "true",
      "east": "false"
          },
          "apply": {
              "model": "pipez:block/item/node",
              "y": 270
          }
      },
      {
          "when": {
              "down": "true",
      "up": "false"
          },
          "apply": {
              "model": "pipez:block/item/node",
              "x": 90
          }
      },
      {
          "when": {
              "up": "true",
      "down": "false"
          },
          "apply": {
              "model": "pipez:block/item/node",
              "x": 270
          }
      },
  
  {
          "when": {
              "north": "true",
      "south": "true"
          },
          "apply": {
              "model": "pipez:block/item/line"
          }
      },
      {
          "when": {
              "east": "true",
      "west": "true"
          },
          "apply": {
              "model": "pipez:block/item/line",
              "y": 90
          }
      },
      {
          "when": {
              "down": "true",
      "up": "true"
          },
          "apply": {
              "model": "pipez:block/item/line",
              "x": 90
          }
      },
      {
          "when": {
              "up": "false",
      "down": "false",
              "west": "false",
      "east": "false",
      "south": "false",
      "north": "false"
          },
          "apply": {
              "model": "pipez:block/item/core"
          }
      }
  
  
  ]
}